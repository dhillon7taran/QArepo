package coreJava.loopstmts;

public class DoWhileLoopDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i=1;
		while(i<=10)
		{
			System.out.println(i);
			
			i++;
			
		}
		
		System.out.println();
		
		int j=11;
		while(j<=10)
		{
			System.out.println(j);
			
		j++;
			
		}
	}

}
