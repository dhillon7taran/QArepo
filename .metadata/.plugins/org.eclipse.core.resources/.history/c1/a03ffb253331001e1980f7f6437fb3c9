package collections;


import java.util.Set;
import java.util.TreeSet;

public class TreeSet1Demo {
	
	public static void main(String[] args)
	{
		//ArrayList<String> nlist=new ArrayList<String>();
		
		Set<String> nameslist=new TreeSet<String>();
		
		nameslist.add("Nancy");
		nameslist.add(1, "Alex");
		nameslist.add("Stephen");
		nameslist.add("Daniel");
		nameslist.add("John");
		
	//	System.out.println(nameslist.get(4));
		
		for(String  x:nameslist)
		{
			System.out.println(x);
		}
		
		System.out.println();
		
		nameslist.remove(3);
		
		for(String  x:nameslist)
		{
			System.out.println(x);
		}
		
		System.out.println();
		
		nameslist.set(3, "Den");
		nameslist.set(1, "Stephen J");
		
		for(String  x:nameslist)
		{
			System.out.println(x);
		}
		
		System.out.println();
		
		System.out.println(nameslist.contains("Den"));
	}

}
